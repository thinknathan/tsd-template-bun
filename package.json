{
	"name": "tsd-template-bun",
	"version": "1.0.0",
	"description": "A minimalist Defold TypeScript template for Bun",
	"keywords": [
		"typescript",
		"lua",
		"defold"
	],
	"author": "TS-Defold Contributors (https://ts-defold.dev/)",
	"license": "MIT",
	"engines": {
		"bun": ">=1.0.0"
	},
	"scripts": {
		"resolve": "library resolve",
		"build": "tstl",
		"lint": "eslint src",
		"prettier": "prettier \"./**/*.{yml,json,ts,md,d.ts}\" --write",
		"dev": "tstl --watch"
	},
	"devDependencies": {
		"@ts-defold/library": "^0.1.1",
		"@ts-defold/tstl-userdata-sugar": "^1.0.1",
		"@ts-defold/types": "^1.2.51",
		"@typescript-to-lua/language-extensions": "^1.19.0",
		"eslint": "^9.0.0",
		"prettier": "^3.1.0",
		"tstl-export-to-global": "^1.0.1",
		"tstl-trim-extensions": "^1.0.3",
		"typescript": "5.5.2",
		"typescript-eslint": "^8.0.0",
		"typescript-to-lua": "~1.26.0",
		"typescript-tstl-plugin": "^0.3.2"
	}
}
